public class NesneTuretme {
    public static void main(String[] args) {
        Insan osman = new Insan();
        osman.boy = 1.78;
        osman.cinsiyet = "Erkek";
        osman.yas = 21;
        
        System.out.println("Boy: " + osman.boy + "\nCinsiyet: " + osman.cinsiyet + "\nYa≈ü: " + osman.yas);
    }
}
